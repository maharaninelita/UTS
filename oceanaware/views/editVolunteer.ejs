<h2>Edit Volunteer</h2>
<form action="/editVolunteer/<%= volunteer.id %>" method="POST">
    <div class="form-group">
        <label for="name">Name:</label>
        <input type="text" class="form-control" id="name" name="name" value="<%= volunteer.name %>" required>
    </div>
    <div class="form-group">
        <label for="contact">Contact:</label>
        <input type="text" class="form-control" id="contact" name="contact" value="<%= volunteer.contact %>" required>
    </div>
    <button type="submit" class="btn btn-primary">Update Volunteer</button>
</form>
